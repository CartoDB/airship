{
  "name": "@carto/airship",
  "version": "1.0.0-alpha.22",
  "private": false,
  "author": {
    "name": "CARTO",
    "email": "frontend@carto.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CartoDB/airship.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/CartoDB/airship/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/airship.js",
  "dependencies": {
    "d3-array": "^1.2.1",
    "d3-axis": "^1.0.8",
    "d3-color": "^1.0.3",
    "d3-dispatch": "^1.0.3",
    "d3-interpolate": "^1.1.6",
    "d3-scale": "^2.0.0",
    "d3-selection": "^1.3.0",
    "d3-shape": "^1.2.0",
    "d3-transition": "^1.1.1",
    "emotion": "^9.0.1",
    "polished": "^1.9.2",
    "react-day-picker": "^7.1.4",
    "react-emotion": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-external-helpers": "^7.0.0-beta.44",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.44",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@storybook/addon-actions": "^4.0.0-alpha.1",
    "@storybook/addon-links": "^4.0.0-alpha.1",
    "@storybook/addons": "^4.0.0-alpha.1",
    "@storybook/react": "^4.0.0-alpha.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.3",
    "catalog": "^3.2.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "~4.19.1",
    "eslint-config-airbnb": "~16.1.0",
    "eslint-plugin-import": "~2.10.0",
    "eslint-plugin-jsx-a11y": "~6.0.3",
    "eslint-plugin-react": "~7.7.0",
    "gzip-size-cli": "^2.1.0",
    "jest": "^22.4.3",
    "jest-fetch-mock": "^1.4.2",
    "jest-styled-components": "^5.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^4.0.0-beta.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-css-only": "^0.4.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^3.0.0",
    "source-map-explorer": "^1.5.0",
    "styled-components": "^3.1.6"
  },
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "styled-components": "^3.1.6"
  },
  "scripts": {
    "analyze": "source-map-explorer dist/airship.*",
    "size": "node -e \"process.stdout.write('gzip size: ')\" && gzip-size dist/airship.js",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch --env=jsdom",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "styleguide": "catalog start docs",
    "styleguide:build": "catalog build docs",
    "build": "NODE_ENV=production rollup -c -m",
    "start": "start-storybook -p 9009 -c config/.storybook",
    "storybook:build": "build-storybook -c config/.storybook",
    "prepublishOnly": "npm run build",
    "release": "npm version patch",
    "release:minor": "npm version minor",
    "release:major": "npm version major",
    "postversion": "git push origin master --follow-tags"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/config/jest.setup.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "modules": "commonjs",
              "useBuiltIns": "usage",
              "debug": false
            }
          ],
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-proposal-object-rest-spread",
          "@babel/plugin-proposal-class-properties"
        ]
      }
    }
  }
}

{
  "name": "@carto/react-airship-ui",
  "version": "3.0.0-alpha5",
  "description": "Airship CARTO API utilities",
  "main": "dist/asreactui.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx}' --fix",
    "dev": "webpack --watch",
    "build": "rm -rf dist && webpack --config webpack.prod.js",
    "publish": "npm publish --access public",
    "publish:beta": "npm publish --tag beta --access public",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "publish-storybook": "build-storybook && cd storybook-static && npx vercel --prod --name 'react-airship-ui'",
    "local-link": "yarn link"
  },
  "author": "CARTO Dev Team",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@babel/core": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-essentials": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/react": "^6.0.27",
    "babel-loader": "^8.1.0",
    "echarts": "^4.9.0",
    "echarts-for-react": "^2.0.16",
    "jest": "23.6.0",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-is": "^17.0.1",
    "webpack": "^4.27.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {}
}
